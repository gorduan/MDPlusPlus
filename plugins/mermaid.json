{
  "framework": "mermaid",
  "version": "10.6.0",
  "author": "Mermaid Team",
  "description": "Mermaid diagram support - flowcharts, sequence diagrams, class diagrams, etc.",
  "js": [
    "https://cdn.jsdelivr.net/npm/mermaid@10/dist/mermaid.min.js"
  ],
  "init": "mermaid.initialize({ startOnLoad: true, theme: 'dark' });",
  "codeBlockLanguages": ["mermaid"],
