        "error": ["admonition-danger"],
        "success": ["admonition-success"],
        "question": ["admonition-question"],
        "quote": ["admonition-quote"],
        "example": ["admonition-example"],
        "abstract": ["admonition-abstract"],
        "bug": ["admonition-bug"]
      }
    },
    "note": {
      "tag": "div",
      "classes": ["admonition", "admonition-note"],
      "icon": "info-circle",
