{
  "framework": "bootstrap",
  "version": "5.3.0",
  "author": "Bootstrap Team",
  "description": "Bootstrap 5 components for MD++",
  "css": [
    "https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css"
  ],
  "js": [
    "https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"
  ],
  "components": {
    "card": {
      "tag": "div",
      "classes": ["card"],
      "allowNesting": true,
      "structure": {
        "header": {
          "tag": "div",
          "classes": ["card-header"]
        },
        "body": {
          "tag": "div",
          "classes": ["card-body"]
        },
        "footer": {
          "tag": "div",
          "classes": ["card-footer"]
        }
      }
    },
    "alert": {
      "tag": "div",
      "classes": ["alert"],
      "variants": {
        "primary": ["alert-primary"],
        "secondary": ["alert-secondary"],
        "success": ["alert-success"],
        "danger": ["alert-danger"],
        "warning": ["alert-warning"],
        "info": ["alert-info"],
        "light": ["alert-light"],
        "dark": ["alert-dark"]
      }
    },
    "badge": {
      "tag": "span",
      "classes": ["badge"],
      "variants": {
        "primary": ["bg-primary"],
        "secondary": ["bg-secondary"],
        "success": ["bg-success"],
        "danger": ["bg-danger"],
        "warning": ["bg-warning", "text-dark"],
        "info": ["bg-info", "text-dark"],
        "light": ["bg-light", "text-dark"],
        "dark": ["bg-dark"]
      }
    },
    "button": {
      "tag": "button",
      "classes": ["btn"],
      "variants": {
        "primary": ["btn-primary"],
        "secondary": ["btn-secondary"],
        "success": ["btn-success"],
        "danger": ["btn-danger"],
        "warning": ["btn-warning"],
        "info": ["btn-info"],
        "outline-primary": ["btn-outline-primary"],
        "outline-secondary": ["btn-outline-secondary"]
      }
    },
    "container": {
      "tag": "div",
      "classes": ["container"],
      "variants": {
        "fluid": ["container-fluid"],
        "sm": ["container-sm"],
        "md": ["container-md"],
        "lg": ["container-lg"],
        "xl": ["container-xl"]
      }
    },
    "row": {
      "tag": "div",
      "classes": ["row"],
      "allowNesting": true
    },
    "col": {
      "tag": "div",
      "classes": ["col"],
      "variants": {
        "1": ["col-1"],
        "2": ["col-2"],
        "3": ["col-3"],
        "4": ["col-4"],
        "5": ["col-5"],
        "6": ["col-6"],
        "7": ["col-7"],
        "8": ["col-8"],
        "9": ["col-9"],
        "10": ["col-10"],
        "11": ["col-11"],
        "12": ["col-12"],
        "auto": ["col-auto"]
      }
    },
    "accordion": {
      "tag": "div",
      "classes": ["accordion"],
      "allowNesting": true
    },
    "accordion-item": {
      "tag": "div",
      "classes": ["accordion-item"]
    },
    "list-group": {
      "tag": "ul",
      "classes": ["list-group"],
      "allowNesting": true
    },
    "list-group-item": {
      "tag": "li",
      "classes": ["list-group-item"]
    },
    "spinner": {
      "tag": "div",
      "classes": ["spinner-border"],
      "variants": {
        "primary": ["text-primary"],
        "secondary": ["text-secondary"],
        "success": ["text-success"],
        "danger": ["text-danger"],
        "grow": ["spinner-grow"]
      }
    },
    "progress": {
      "tag": "div",
      "classes": ["progress"]
    },
    "progress-bar": {
      "tag": "div",
      "classes": ["progress-bar"],
      "variants": {
        "success": ["bg-success"],
        "info": ["bg-info"],
        "warning": ["bg-warning"],
        "danger": ["bg-danger"],
        "striped": ["progress-bar-striped"],
        "animated": ["progress-bar-striped", "progress-bar-animated"]
      }
    },
    "navbar": {
      "tag": "nav",
      "classes": ["navbar"],
      "allowNesting": true,
      "variants": {
        "light": ["navbar-light", "bg-light"],
        "dark": ["navbar-dark", "bg-dark"],
        "expand-lg": ["navbar-expand-lg"],
        "expand-md": ["navbar-expand-md"],
        "fixed-top": ["fixed-top"],
        "fixed-bottom": ["fixed-bottom"]
      }
    },
    "navbar-brand": {
      "tag": "a",
      "classes": ["navbar-brand"]
    },
    "nav": {
      "tag": "ul",
      "classes": ["navbar-nav"],
      "allowNesting": true
    },
    "nav-item": {
      "tag": "li",
      "classes": ["nav-item"]
    },
    "nav-link": {
      "tag": "a",
      "classes": ["nav-link"]
    },
    "tabs": {
      "tag": "ul",
      "classes": ["nav", "nav-tabs"],
      "allowNesting": true,
      "specialParser": "tabs"
    },
    "tab-content": {
      "tag": "div",
      "classes": ["tab-content"],
      "allowNesting": true
    },
    "tab-pane": {
      "tag": "div",
      "classes": ["tab-pane", "fade"],
      "variants": {
        "active": ["show", "active"]
      }
    },
    "carousel": {
      "tag": "div",
      "classes": ["carousel", "slide"],
      "allowNesting": true,
      "variants": {
        "fade": ["carousel-fade"]
      }
    },
    "carousel-inner": {
      "tag": "div",
      "classes": ["carousel-inner"],
      "allowNesting": true
    },
    "carousel-item": {
      "tag": "div",
      "classes": ["carousel-item"],
      "variants": {
        "active": ["active"]
      }
    },
    "modal": {
      "tag": "div",
      "classes": ["modal", "fade"],
      "allowNesting": true,
      "variants": {
        "lg": ["modal-lg"],
        "sm": ["modal-sm"],
        "xl": ["modal-xl"],
        "centered": ["modal-dialog-centered"],
        "scrollable": ["modal-dialog-scrollable"]
      }
    },
    "modal-dialog": {
      "tag": "div",
      "classes": ["modal-dialog"]
    },
    "modal-content": {
      "tag": "div",
      "classes": ["modal-content"]
    },
    "modal-header": {
      "tag": "div",
      "classes": ["modal-header"]
    },
    "modal-body": {
      "tag": "div",
      "classes": ["modal-body"]
    },
    "modal-footer": {
      "tag": "div",
      "classes": ["modal-footer"]
    },
    "breadcrumb": {
      "tag": "nav",
      "classes": [],
      "allowNesting": true
    },
    "breadcrumb-list": {
      "tag": "ol",
      "classes": ["breadcrumb"]
    },
    "breadcrumb-item": {
      "tag": "li",
      "classes": ["breadcrumb-item"],
      "variants": {
        "active": ["active"]
      }
    },
    "dropdown": {
      "tag": "div",
      "classes": ["dropdown"],
      "allowNesting": true
    },
    "dropdown-toggle": {
      "tag": "button",
      "classes": ["btn", "dropdown-toggle"]
    },
    "dropdown-menu": {
      "tag": "ul",
      "classes": ["dropdown-menu"],
      "allowNesting": true
    },
    "dropdown-item": {
      "tag": "li",
      "classes": []
    },
    "table": {
      "tag": "table",
      "classes": ["table"],
      "variants": {
        "striped": ["table-striped"],
        "bordered": ["table-bordered"],
        "hover": ["table-hover"],
        "dark": ["table-dark"],
        "sm": ["table-sm"]
      }
    },
    "form-group": {
      "tag": "div",
      "classes": ["mb-3"]
    },
    "form-control": {
      "tag": "input",
      "classes": ["form-control"],
      "variants": {
        "lg": ["form-control-lg"],
        "sm": ["form-control-sm"]
      }
    },
    "form-label": {
      "tag": "label",
      "classes": ["form-label"]
    },
    "toast": {
      "tag": "div",
      "classes": ["toast"],
      "allowNesting": true
    },
    "toast-header": {
      "tag": "div",
      "classes": ["toast-header"]
    },
    "toast-body": {
      "tag": "div",
      "classes": ["toast-body"]
    },
    "offcanvas": {
      "tag": "div",
      "classes": ["offcanvas"],
      "variants": {
        "start": ["offcanvas-start"],
        "end": ["offcanvas-end"],
        "top": ["offcanvas-top"],
        "bottom": ["offcanvas-bottom"]
      }
    },
    "offcanvas-header": {
      "tag": "div",
      "classes": ["offcanvas-header"]
    },
    "offcanvas-body": {
      "tag": "div",
      "classes": ["offcanvas-body"]
    }
  }
}
