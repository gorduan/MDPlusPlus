{
  "id": "material-icons",
  "framework": "material-icons",
  "name": "Material Design Icons",
  "version": "1.0.0",
  "author": "MD++ Team",
  "description": "Google Material Design Icons integration. Use ![icon](google:icon-name) syntax to insert icons.",
  "license": "MIT",

  "type": "parser",

  "parser": {
    "entry": "./parser.ts",
    "phase": "remark",
    "priority": 110
  },

  "assets": {
    "css": [
      "./vendor/material-icons.css"
    ]
  },

  "icons": {
    "prefix": "google:",
    "tag": "span",
    "baseClasses": ["material-icons"],
    "variants": {
      "outlined": ["material-icons-outlined"],
      "round": ["material-icons-round"],
      "sharp": ["material-icons-sharp"],
      "two-tone": ["material-icons-two-tone"]
    },
    "sizes": {
      "small": { "class": "md-18", "style": "font-size: 18px;" },
      "medium": { "class": "md-24", "style": "font-size: 24px;" },
      "large": { "class": "md-36", "style": "font-size: 36px;" },
      "x-large": { "class": "md-48", "style": "font-size: 48px;" }
    },
    "commonIcons": [
      "home", "search", "menu", "close", "settings", "person", "mail",
      "favorite", "star", "delete", "edit", "add", "remove", "check",
      "arrow_back", "arrow_forward", "refresh", "download", "upload",
      "visibility", "visibility_off", "lock", "lock_open", "info",
      "warning", "error", "help", "notifications", "shopping_cart",
      "account_circle", "face", "thumb_up", "thumb_down", "share",
      "content_copy", "content_paste", "save", "print", "send",
      "attach_file", "link", "image", "photo_camera", "videocam",
      "mic", "volume_up", "volume_off", "play_arrow", "pause", "stop",
      "skip_previous", "skip_next", "fast_forward", "fast_rewind",
      "folder", "folder_open", "file_copy", "description", "code",
      "terminal", "bug_report", "build", "extension", "lightbulb"
    ]
  },

  "settings": {
    "defaults": {
      "defaultStyle": "filled",
      "defaultSize": "24px",
      "customClass": ""
    }
  },

  "components": {},

  "categories": ["icons", "ui"],
  "keywords": ["material", "icons", "google", "symbols", "ui"],

  "minAppVersion": "1.0.0"
}
